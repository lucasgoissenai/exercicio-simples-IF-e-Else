
let numero= prompt("Informe sua idade:");
if (numero < 18){
console.log("Menor de idade");
}
else if (numero > 18){
console.log("Maior de idade");
alert("Maior de idade")
}
else if (numero = 18){
console.log("Maior de idade");
alert("Maior de idade");
}
else {
console.log("Favor informar uma idade válida");
alert("Favor informar uma idade válida");
}
